{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "../../dist/out-tsc",
    "esModuleInterop": true,
    "target": "es2015",
    "jsx": "preserve",
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [{ "name": "next" }],
    "noEmit": false
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
